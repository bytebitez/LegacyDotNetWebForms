      "UpstreamHttpMethod": [ "Get", "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/api/cart/{cartId}/items",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cart-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/cart/{cartId}/items",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/cart/{cartId}/items/{productId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cart-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/cart/{cartId}/items/{productId}",
      "UpstreamHttpMethod": [ "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/api/cart/{cartId}/total",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cart-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/cart/{cartId}/total",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/orders",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "order-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/orders",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/orders/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "order-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/orders/{id}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/orders/user/{username}",
      "DownstreamScheme": "http",
